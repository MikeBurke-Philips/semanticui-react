{
  "name": "semanticui-react",
  "version": "0.2.2",
  "description": "Semanticui React Component",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomitrescak/semanticui-react.git"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9010",
    "publish-storybook": "bash .scripts/publish_storybook.sh"
  },
  "devDependencies": {
    "jquery": "^2.2.3",
    "json-loader": "^0.5.4",
    "marked": "^0.3.6",
    "raw-loader": "^0.5.1",
    "semantic-ui-css": "^2.2.2",
    "style-loader": "^0.13.1"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "jss": "^5.3.0",
    "jss-nested": "^2.0.0",
    "react": "^15.0.0"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
  },
  "typings": "./dist/index.d.ts"
}
